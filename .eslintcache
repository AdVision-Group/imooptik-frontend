/SearchButton>\n                </SearchContainer>\n                <CartContainer>\n                    <HeadingContainer>\n                        <h3>Vybran poloky</h3>\n                        {selectedProduct !== null && <UnselectButton onClick={() => selectProduct(null)}>Odznai</UnselectButton>}\n                    </HeadingContainer>\n                    <CartTable>\n\n                        <CartTableHead>\n                            <TableCol>#</TableCol>\n                            <TableCol>Produkt</TableCol>\n                            <TableCol>oovky</TableCol>\n                            <TableCol>Cena</TableCol>\n                            <TableCol>Ks</TableCol>\n                            <TableCol>Zava v %</TableCol>\n                            {/* <TableCol>Monosti</TableCol> */}\n                        </CartTableHead>\n                        {cart.map((item, idx) => {\n                            if (item.product.type === 3 || item.product.type === 4 || item.product.type === 5 || item.product.type === 6) return false\n                            return (\n                                <CartLensesRow\n                                    key={idx}\n                                    idx={idx}\n                                    item={item}\n                                />\n                            )\n                        })}\n\n                        {cart.length === 0 && <CartParagraph>Nie s vybran iadn produkty</CartParagraph>}\n                    </CartTable>\n                    <ButtonOptions>\n                        <NextButton onClick={createCombinedProducts}>\n                            Dokoni objednvku\n                        </NextButton>\n                    </ButtonOptions>\n                </CartContainer>\n            </HeaderContainer>\n\n            <SelectLensesContainer>\n                <LensesTableContainer>\n                    <LensesTableHead>\n                        <TableCol>eanKd</TableCol>\n